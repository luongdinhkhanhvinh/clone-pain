{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "concurrently \"pnpm run dev:frontend\" \"pnpm run dev:backend\"",
    "dev:frontend": "next dev",
    "dev:backend": "cd server && pnpm run dev",
    "lint": "next lint",
    "start": "next start",
    "server:install": "cd server && pnpm install",
    "db:generate": "cd server && pnpm drizzle-kit generate",
    "db:push": "cd server && pnpm drizzle-kit push",
    "db:migrate": "cd server && pnpm drizzle-kit migrate",
    "db:seed": "cd server && pnpm run db:seed",
    "setup": "pnpm install && pnpm run server:install",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:dev:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down --remove-orphans && docker system prune -f"
  },
